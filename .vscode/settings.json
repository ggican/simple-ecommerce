{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "cSpell.enabled": true,
  "cSpell.words": [
    "sibiraj",
    "esbenp",
    "yzhang",
    "foxundermoon",
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml",
    "michelemelluso",
    "tiket",
    "compoments",
    "autofetch",
    "uuidv4"
  ],
  "cSpell.userWords": ["gapi", "hookform", "resultvalues"],
  "claude.lang": "en",
  "cmake.configureOnOpen": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "always",
    "source.fixAll.stylelint": "explicit"
  },
  "editor.defaultFormatter": "michelemelluso.code-beautifier",
  "editor.guides.bracketPairs": "active",
  "editor.suggestSelection": "first",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.triggerExpansionOnTab": true,
  "eslint.format.enable": true,
  "files.exclude": {
    ".npm-offline-cache": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    ".hg": true,
    "bower_compoments": true
  },
  "git.autofetch": true,
  "git.suggestSmartCommit": false,
  "html.format.enable": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "redhat.telemetry.enabled": true,
  "sort-imports.cache-package-json-config-checks": false,
  "sort-imports.default-sort-style": "eslint",
  "sort-imports.ignore-type-defs": false,
  "sort-imports.on-save": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#0e0f10",
    "activityBar.border": "#0b1015",
    "editor.background": "#1b1e23",
    "editorGroupHeader.noTabsBackground": "#0e0f10",
    "editorGroupHeader.tabsBackground": "#0e0f10",
    "list.activeSelectionBackground": "#0e0f10",
    "list.inactiveSelectionBackground": "#161a1d",
    "panel.border": "#0e0f10",
    "sideBar.background": "#0e0f10",
    "sideBarSectionHeader.background": "#0e0f10",
    "statusBar.border": "#0e0f10",
    "tab.border": "#0b1015",
    "tab.inactiveBackground": "#111213",
    "titleBar.border": "#0e0f10"
  },
  "workbench.colorTheme": "Monokai",
  "workbench.iconTheme": "vscode-icons",
  "[tailwindcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
